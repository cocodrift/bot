{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": ".vercel/functions/api",
      "methods": ["GET", "POST"]
    }
  ],
  "builds": [
    {
      "src": "backend/app.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "maxDuration": {
      "duration": 60
    }
  }
}
