{
  "version": 2,
  "functions": {
    "api": {
      "runtime": {
        "type": "@vercel/node",
        "version": "1.0.0"
      },
      "memory": 512,
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "backend/app.js"
    }
  ]
}
